{
  "Version": "2012-10-17",
  "Statement": [ {
      "Effect": "Allow",
      "Action": [
        "logs:*"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    }, {
      "Effect": "Allow",
      "Action": [
        "kinesis:DescribeStream",
        "kinesis:GetRecords",
        "kinesis:GetShardIterator",
        "kinesis:ListStreams"
      ],
      "Resource": "arn:aws:kinesis:*:*:*"
  } ]
}
